<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_to_kegiatanadmin"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:padding="5.20dp"
    tools:context=".KegiatanAdminActivity">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/list_kegiatan_admin"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingBottom="19.20dp"
        android:scrollbars="none"
        tools:listitem="@layout/activity_list_kegiatan_admin"/>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|right"
        android:layout_marginEnd="38.40dp"
        android:layout_marginRight="16.00dp"
        android:layout_marginBottom="38.40dp"
        android:src="@android:drawable/ic_input_add"
        app:backgroundTint="@color/primaryVariant"
        tools:ignore="SpeakableTextPresentCheck" />
</FrameLayout>